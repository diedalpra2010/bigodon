{% extends 'base.html.twig' %}

{% block body %}
    <h1>Agendamento</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agendamento.id }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ agendamento.nome }}</td>
            </tr>
            <tr>
                <th>Telefone</th>
                <td>{{ agendamento.telefone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ agendamento.email }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('agendamento_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
